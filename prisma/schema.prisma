generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Member {
  id          Int      @id @default(autoincrement())
  team        String
  name        String
  description String
  cover       Bytes // This will store the compressed image binary data
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
